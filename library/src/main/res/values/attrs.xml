<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DiagonalImageView">
        <attr name="di_position" format="enum">
            <enum name="top" value="1" />
            <enum name="right" value="2" />
            <enum name="bottom" value="4" />
            <enum name="left" value="8" />
        </attr>
        <attr name="di_direction" format="enum">
            <enum name="top" value="1" />
            <enum name="right" value="2" />
            <enum name="bottom" value="4" />
            <enum name="left" value="8" />
        </attr>
        <attr name="di_overlap" format="dimension" />
        <attr name="di_borderSize" format="dimension" />
        <attr name="di_borderColor" format="color" />
        <attr name="di_borderEnabled" format="boolean" />
    </declare-styleable>

</resources>